{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"customDuotone": false,
			"customGradient": false,
			"defaultDuotone": false,
			"defaultGradients": false,
			"defaultPalette": false,
			"palette": [
				{
					"color": "#E50A0A",
					"name": "Primaire foncé",
					"slug": "primary-dark"
				},
				{
					"color": "#FF0A0A",
					"name": "Primaire",
					"slug": "primary"
				},
				{
					"color": "#F9E1DC",
					"name": "Primaire léger",
					"slug": "primary-light"
				},
				{
					"color": "#212020",
					"name": "Texte courant",
					"slug": "contrast"
				},
				{
					"color": "#6b6b6b",
					"name": "Texte léger",
					"slug": "contrast-light"
				},
				{
					"color": "#CCCDC8",
					"name": "Texte très léger",
					"slug": "contraster-lighter"
				},
				{
					"color": "#FFFFFF",
					"name": "Texte fond sombre",
					"slug": "contrast-inverted"
				},
				{
					"color": "#FAFCF5",
					"name": "Fond",
					"slug": "base"
				},
				{
					"color": "#E6DBCE",
					"name": "Fond sombre",
					"slug": "base-darker"
				},
				{
					"color": "#FBFCF6",
					"name": "Fond clair",
					"slug": "base-light"
				}
			]
		},
		"custom": {
			"depth": {
				"back": 0,
				"content": 10,
				"front": 50
			},
			"border": {
				"radius": {
					"lg": "3rem",
					"md": "1rem",
					"menu": "15rem",
					"sm": ".5rem"
				}
			}
		},
		"layout": {
			"allowEditing": true,
			"contentSize": "41rem",
			"wideSize": "55rem"
		},
		"spacing": {
			"spacingScale": {
				"increment": 0.75,
				"operator": "*",
				"steps": 9,
				"unit": "rem"
			},
			"spacingSizes": []
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "Georgia, serif",
					"name": "Défaut",
					"slug": "default"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Neutronic",
							"fontWeight": "400",
							"src": "file:./assets/fonts/Neutronic Regular.woff"
						},
						{
							"fontFamily": "Neutronic",
							"fontWeight": "600",
							"src": "file:./assets/fonts/Neutronic Semibold.woff"
						}
					],
					"fontFamily": "\"Neutronic\", sans-serif",
					"name": "Décroché",
					"slug": "highlight"
				}
			],
			"fontSizes": [
				{
					"name": "Titre: Page 1",
					"size": ".8701em",
					"slug": "476"
				},
				{
					"name": "Titre: Page 2",
					"size": ".87em",
					"slug": "475"
				},
				{
					"name": "Titre: Page 3",
					"size": ".72em",
					"slug": "300"
				},
				{
					"name": "Sur titre",
					"size": ".64em",
					"slug": "250"
				},
				{
					"name": "Date",
					"size": ".6401em",
					"slug": "251"
				},
				{
					"name": "Sous titre",
					"size": ".59em",
					"slug": "100"
				},
				{
					"name": "Titre: Article 1",
					"size": "2.6em",
					"slug": "750"
				},
				{
					"name": "Titre: Article 2",
					"size": "2.3em",
					"slug": "700"
				},
				{
					"name": "Titre: Article 3",
					"size": "1.7em",
					"slug": "650"
				},
				{
					"name": "Texte: Chapeau",
					"size": "1.2em",
					"slug": "600"
				},
				{
					"name": "Texte: Courant",
					"size": ".85em",
					"slug": "450"
				},
				{
					"name": "Annotation: Titre",
					"size": ".6em",
					"slug": "200"
				},
				{
					"name": "Annotation: Liens / contenu",
					"size": ".8em",
					"slug": "400"
				},
				{
					"name": "Crédits",
					"size": ".51em",
					"slug": "050"
				}
			],
			"fontStyle": true,
			"fontWeight": false,
			"letterSpacing": true,
			"textAlign": true,
			"textDecoration": true,
			"textTransform": true
		}
	},
	"styles": {
		"blocks": {
			"core/heading": {
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--base-darker)",
			"text": "var(--wp--preset--color--contrast)"
		},
		"css": "body { overflow-x: hidden }",
		"elements": {
			"button": {
				"color": {
					"background": "transparent",
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--highlight)"
				}
			},
			"heading": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					}
				},
				"typography": {
					"fontWeight": "400"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary-dark)"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--default)",
			"fontSize": "1rem",
			"fontStyle": "normal",
			"fontWeight": "400"
		}
	},
	"customTemplates": [
		{
			"name": "pages--jury",
			"postTypes": [ "page", "jury" ],
			"title": "Template Jurés"
		},
		{
			"name": "pages--graduate",
			"postTypes": [
				"page"
			],
			"title": "Template Lauréats"
		},
		{
			"name": "pages--no-gif",
			"postTypes": [
				"page"
			],
			"title": "Page sans en-tête animé"
		}
	],
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 3
}